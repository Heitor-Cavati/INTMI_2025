        "#27\n",
        "senha_correta = \"python123\"\n",
        "max_tentativas = 3\n",
        "\n",
        "for tentativa in range(1, max_tentativas + 1):\n",
        "  senha_digitada = input(f\"Tentativa {tentativa}/{max_tentativas}: Digite a senha: \")\n",
        "\n",
        "  if senha_digitada == senha_correta:\n",
        "    print(\"Bem-vindo(a) ao sistema!\")\n",
        "    break\n",
        "  else:\n",
        "    print(\"Senha incorreta.\")\n",
        "\n",
        "  if tentativa == max_tentativas:\n",
        "    print(\"Número máximo de tentativas excedido. Acesso bloqueado.\")"
      ],
      "metadata": {
        "id": "xnEZDRq5dLed"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#28\n",
        "soma_positivos = 0\n",
        "\n",
        "print(\"Digite 10 números:\")\n",
        "for i in range(1, 11):\n",
        "  numero = int(input(f\"Número {i}: \"))\n",
        "  if numero < 0:\n",
        "    print(\"Número negativo ignorado.\")\n",
        "    continue\n",
